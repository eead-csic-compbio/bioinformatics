@article{
   Bates1999,
   Author = {Bates, P. A. and Sternberg, M. J.},
   Title = {Model building by comparison at CASP3: Using expert knowledge and computer automation},
   Journal = {Proteins},
   Volume = {37},
   Number = {S3},
   Pages = {47-54},
   Abstract = {Ten models were constructed for the comparative modeling section of the Critical Assessment of Techniques for Protein Structure Prediction-3 (CASP3). Sequence identity between each target and the best possible parent(s) ranged between 12% and 64%. The modeling protocol is a mixture of automated computer algorithms with human intervention at certain critical stages. In particular, intervention is required to check sequence alignments and the selection of parameters for various computer programs. Seven of the targets were constructed from single- parent templates, and three were constructed from multiple parents. The reasons for such a high ratio of modeling from single parents only are discussed. Models constructed from multiple parents were found to be more accurate than models constructed from single parents only. A novel loop-modeling algorithm is presented that consists of fragment database searches, several fragment libraries, and mean-field calculations on representative fragment candidates. Proteins Suppl 1999;3:47-54. Copyright 1999 Wiley-Liss, Inc.},
 Year = {1999} }


